<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.yf.oa.dao.SettingMapper">

	<select id="listGroup" resultType="HashMap">
		SELECT name FROM `group`
	</select>


	<select id="listObs" parameterType="Integer" resultType="HashMap">
		SELECT * FROM `oa_obs` where pid = ${value}
	</select>

</mapper>
   
